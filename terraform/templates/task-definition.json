[
  {
    "cpu": 256,
    "essential": true,
    "image": "${image_url}",
    "memory": 1024,
    "name": "${container_name}",
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 8080
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/spring-boot-test/app",
        "awslogs-region": "${log_group_region}",
        "awslogs-stream-prefix": "hello-ecs"
      }
    }
  }
]
